<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_main"
    app:startDestination="@id/BFragment">
    <fragment
        android:id="@+id/initialFragment"
        android:name="com.esaudev.gamesgarden.ui.navigation_states.InitialFragment"
        android:label="fragment_initial"
        tools:layout="@layout/fragment_initial" >
        <action
            android:id="@+id/action_initialFragment_to_BFragment"
            app:destination="@id/BFragment" />

        <deepLink app:uri="https://codewithesau.com/{productId}"/>
        <argument android:name="productId" app:argType="string" android:defaultValue="test"/>
    </fragment>
    <fragment
        android:id="@+id/finalFragment"
        android:name="com.esaudev.gamesgarden.ui.navigation_states.FinalFragment"
        android:label="fragment_final"
        tools:layout="@layout/fragment_final" />
    <fragment
        android:id="@+id/BFragment"
        android:name="com.esaudev.gamesgarden.ui.navigation_states.recyclerview.BFragment"
        android:label="fragment_b"
        tools:layout="@layout/fragment_b" >
        <action
            android:id="@+id/action_BFragment_to_finalFragment"
            app:destination="@id/finalFragment" />
        <argument
            android:name="fragmentB"
            app:argType="string"
            app:nullable="false"/>
    </fragment>
</navigation>